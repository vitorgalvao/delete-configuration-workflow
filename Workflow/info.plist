				<string>function envVar(varName) {
  return $.NSProcessInfo
    .processInfo
    .environment
    .objectForKey(varName).js
}

function isNotDir(pathString) {
  return $.NSFileManager.defaultManager
    .attributesOfItemAtPathError(pathString, undefined)
    .js["NSFileType"].js !== "NSFileTypeDirectory"
}

function runCommand(...arguments) {
  const task = $.NSTask.alloc.init
  const stdout = $.NSPipe.pipe

  task.executableURL = $.NSURL.fileURLWithPath("/usr/bin/env")
  task.arguments = arguments
  task.standardOutput = stdout
  task.launchAndReturnError(false)

  const dataOut = stdout.fileHandleForReading.readDataToEndOfFileAndReturnError(false)
  const stringOut = $.NSString.alloc.initWithDataEncoding(dataOut, $.NSUTF8StringEncoding).js

  return stringOut
}

function pathToItem(path) {
  return {
    uid: path,
    title: $(path).lastPathComponent.js,
    subtitle: $(path).stringByAbbreviatingWithTildeInPath.js,
    arg: path,
    type: "file",
    icon: { path: path, type: "fileicon" }
  }
}

function noResults(searchQuery) {
  return JSON.stringify({ items: [{
    title: "No Results",
    subtitle: `No paths found matching “${searchQuery}”`,
    valid: false
  }]})
}

function run(argv) {
  const searchQuery = argv[0]
  const previousQuery = envVar("previous_query")
  const previousPaths = envVar("previous_paths")

  // If searching the same thing via External Trigger, return early with updated results
  if (previousQuery === searchQuery) {
console.log(previousPaths)
console.log(previousPaths.length)
    if (previousPaths.length === 0) return noResults(searchQuery)

    return JSON.stringify({
      variables: { previous_query: previousQuery, previous_paths: previousPaths },
      items: previousPaths.split("\n").map(path =&gt; pathToItem(path))
    })
  }

  // Find paths
  const ignoredFlags = envVar("ignored_suffixes")
    .split("\n")
    .flatMap(suffix =&gt; ["--exclude", `${suffix}/*`])

  const matchedPaths = runCommand("fd", searchQuery, ...ignoredFlags,
    "--ignore-case", "--unrestricted", "--fixed-strings",
    "--type", "file", "--type", "dir",
    "--search-path", $("~/Library").stringByExpandingTildeInPath.js)
    .split("\n")
    .slice(0, -1) // Remove last (empty) item
    .sort(isNotDir)

  if (matchedPaths.length === 0) return noResults(searchQuery)

  return JSON.stringify({
    variables: { previous_query: searchQuery, previous_paths: matchedPaths.join("\n") },
    items: matchedPaths.map(path =&gt; pathToItem(path))
  })
}</string>
