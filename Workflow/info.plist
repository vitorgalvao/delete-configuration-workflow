				<string>function envVar(varName) {
  return $.NSProcessInfo
    .processInfo
    .environment
    .objectForKey(varName).js
}

function isNotDir(pathString) {
  return $.NSFileManager.defaultManager
    .attributesOfItemAtPathError(pathString, undefined)
    .js["NSFileType"].js !== "NSFileTypeDirectory"
}

function run(argv) {
  const deletedPaths = argv
  const previousPaths = envVar("previous_paths").split("\n")

  return deletedPaths.reduce((allPaths, currentPath) =&gt; {
    // Remove deleted path
    allPaths = allPaths.filter(path =&gt; path !== currentPath)
    // Return early if not a directory
    if (isNotDir(currentPath)) return allPaths
    // Remove child paths
    return allPaths.filter(path =&gt; !path.startsWith(`${currentPath}/`))
  }, previousPaths).join("\n")
}</string>
